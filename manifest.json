{
  "name": "Cookie Monster",
  "version": "1.0",
  "description": "Block annoying 'accept cookies' popups.",
  "icons": {
    "16": "img/cookiemonster16.png",
    "32": "img/cookiemonster32.png",
    "128": "img/cookiemonster128.png"
  },
  "browser_action": {
    "default_icon": "img/cookiemonster32.png"
  },
  "permissions": ["activeTab"],
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["cookiemonster.js"]
    }
  ]
}
